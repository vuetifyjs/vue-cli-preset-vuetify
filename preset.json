{
  "bare": true,
  "useConfigFiles": true,
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-eslint": {
      "config": "standard",
      "lintOn": ["save"]
    },
    "@vue/cli-plugin-router": {
      "historyMode": true
    },
    "vue-cli-plugin-vuetify": {
      "preset": "configure",
      "replaceComponents": false,
      "useTheme": false,
      "useCustomProperties": false,
      "iconFont": "mdi",
      "installFonts": false,
      "useAlaCarte": true,
      "usePolyfill": true,
      "locale": "en"
    }
  },
  "cssPreprocessor": "dart-sass",
  "configs": {
    "vue": {
      "devServer": {
        "disableHostCheck": true
      }
    }
  }
}
